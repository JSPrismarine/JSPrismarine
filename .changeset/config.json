{
    "$schema": "https://unpkg.com/@changesets/config@latest/schema.json",
    "changelog": [
        "@changesets/changelog-github",
        {
            "repo": "JSPrismarine/JSPrismarine"
        }
    ],
    "commit": false,
    "fixed": [
        [
            "{,@jsprismarine/}{,*,!color-parser,!brigadier,!bedrock-data}"
        ]
    ],
    "linked": [],
