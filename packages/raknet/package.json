{
    "name": "@jsprismarine/raknet",
    "version": "0.1.0",
    "description": "Basic RakNet implementation written in TypeScript",
    "sideEffects": false,
    "type": "module",
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "module": "./dist/RakNet.es.js",
    "types": "./dist/RakNet.d.ts",
    "exports": {
        ".": {
            "import": "./dist/RakNet.es.js",
            "module": "./dist/RakNet.es.js",
            "require": "./dist/RakNet.cjs.js",
            "types": "./dist/RakNet.d.ts"
        },
        "./*": {
            "import": "./dist/*.es.js",
            "module": "./dist/*.es.js",
            "require": "./dist/*.cjs.js",
            "types": "./dist/*.d.ts"
        },
        "./package.json": "./package.json"
    },
    "funding": "https://github.com/sponsors/JSPrismarine",
    "scripts": {
        "build": "vite build",
        "typecheck": "tsc -noEmit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JSPrismarine/JSPrismarine.git"
    },
    "keywords": [
        "nodejs",
        "raknet"
    ],
    "author": "JSPrismarine",
    "license": "MIT",
    "homepage": "https://github.com/JSPrismarine/JSPrismarine/tree/master/packages/raknet",
    "dependencies": {
        "@jsprismarine/jsbinaryutils": "5.5.3",
        "pino": "8.19.0"
    },
    "devDependencies": {
        "@types/node": "20.11.30",
        "glob": "10.3.10",
        "typescript": "5.4.3",
        "vite": "5.2.4",
        "vitest": "1.4.0"
    }
}
