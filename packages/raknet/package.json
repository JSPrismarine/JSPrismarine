{
    "name": "@jsprismarine/raknet",
    "version": "0.4.3",
    "description": "Basic RakNet implementation written in TypeScript",
    "sideEffects": false,
    "type": "module",
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "main": "./dist/RakNet.cjs.cjs",
    "module": "./dist/RakNet.es.js",
    "types": "./dist/RakNet.d.ts",
    "exports": {
        ".": {
            "import": [
                "./src/RakNet.ts",
                "./dist/RakNet.es.js"
            ],
            "module": "./dist/RakNet.es.js",
            "require": "./dist/RakNet.cjs.cjs",
            "types": "./dist/RakNet.d.ts"
        },
        "./*": {
            "import": "./dist/*.es.js",
            "module": "./dist/*.es.js",
            "require": "./dist/*.cjs.cjs",
            "types": "./dist/*.d.ts"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "clean": "rimraf dist jsp *.tsbuildinfo vite.config.ts.timestamp-* .tsup .turbo",
        "build": "vite build",
        "build:watch": "vite build --watch",
        "typecheck": "tsc -noEmit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JSPrismarine/JSPrismarine.git"
    },
    "keywords": [
        "nodejs",
        "raknet"
    ],
    "author": "JSPrismarine",
    "license": "MIT",
    "homepage": "https://github.com/JSPrismarine/JSPrismarine/tree/master/packages/raknet",
    "dependencies": {
        "@jsprismarine/jsbinaryutils": "5.5.3"
    },
    "devDependencies": {
        "@types/node": "20.12.7",
        "glob": "10.3.12",
        "rimraf": "5.0.5",
        "typescript": "5.4.5",
        "vite": "5.2.11",
        "vitest": "1.5.3"
    }
}
