{
    "name": "@jsprismarine/minecraft",
    "version": "0.13.1",
    "description": "Minecraft: Bedrock Edition data and related utilities",
    "sideEffects": false,
    "type": "module",
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "module": "./dist/index.es.js",
            "require": "./dist/index.cjs.cjs"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.es.js",
            "module": "./dist/*.es.js",
            "require": "./dist/*.cjs.cjs"
        },
        "./package.json": "./package.json"
    },
    "funding": "https://github.com/sponsors/JSPrismarine",
    "scripts": {
        "clean": "rimraf dist *.tsbuildinfo .tsup .turbo",
        "build": "tsup",
        "build:watch": "tsup --watch",
        "typecheck": "tsc -noEmit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JSPrismarine/JSPrismarine.git"
    },
    "keywords": [
        "protocol-data",
        "minecraft-data",
        "minecraft"
    ],
    "author": "JSPrismarine",
    "license": "MIT",
    "homepage": "https://github.com/JSPrismarine/JSPrismarine/tree/master/packages/minecraft",
    "devDependencies": {
        "@types/node": "22.18.11",
        "@types/semver": "7.7.1",
        "glob": "13.0.0",
        "rimraf": "6.1.2",
        "typescript": "5.9.3",
        "vitest": "4.0.15"
    },
    "dependencies": {
        "@jsprismarine/errors": "workspace:*",
        "@jsprismarine/nbt": "workspace:*"
    }
}
