{
    "name": "@jsprismarine/minecraft",
    "version": "0.12.0",
    "description": "Minecraft: Bedrock Edition data and related utilities",
    "sideEffects": false,
    "type": "module",
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "module": "./dist/index.es.js",
            "require": "./dist/index.cjs.cjs",
            "types": "./dist/index.d.ts"
        },
        "./*": {
            "import": "./dist/*.es.js",
            "module": "./dist/*.es.js",
            "require": "./dist/*.cjs.cjs",
            "types": "./dist/*.d.ts"
        },
        "./package.json": "./package.json"
    },
    "funding": "https://github.com/sponsors/JSPrismarine",
    "scripts": {
        "build": "vite build",
        "typecheck": "tsc -noEmit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JSPrismarine/JSPrismarine.git"
    },
    "keywords": [
        "protocol-data",
        "minecraft-data",
        "minecraft"
    ],
    "author": "JSPrismarine",
    "license": "MIT",
    "homepage": "https://github.com/JSPrismarine/JSPrismarine/tree/master/packages/minecraft",
    "devDependencies": {
        "@types/node": "22.9.0",
        "@types/semver": "7.5.8",
        "glob": "11.0.0",
        "typescript": "5.5.2",
        "vite": "5.4.10",
        "vitest": "2.1.4"
    },
    "dependencies": {
        "@jsprismarine/errors": "workspace:*",
        "@jsprismarine/nbt": "workspace:*"
    }
}
