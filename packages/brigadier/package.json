{
    "name": "@jsprismarine/brigadier",
    "version": "0.12.0",
    "description": "A javascript re-implementation of Mojang's Brigadier library",
    "sideEffects": false,
    "type": "module",
    "files": [
        "dist",
        "LICENSE",
        "CHANGELOG.md",
        "README.md"
    ],
    "main": "./dist/index.cjs.cjs",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "module": "./dist/index.es.js",
            "require": "./dist/index.cjs.cjs",
            "types": "./dist/index.d.ts"
        },
        "./*": {
            "import": "./dist/*.es.js",
            "module": "./dist/*.es.js",
            "require": "./dist/*.cjs.cjs",
            "types": "./dist/*.d.ts"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "clean": "rimraf dist jsp *.tsbuildinfo vite.config.ts.timestamp-* .tsup .turbo",
        "build": "vite build",
        "build:watch": "vite build --watch",
        "typecheck": "tsc -noEmit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JSPrismarine/JSPrismarine.git"
    },
    "keywords": [
        "nodejs",
        "brigadier",
        "minecraft",
        "minecraft-commands",
        "commands"
    ],
    "author": "JSPrismarine",
    "license": "MIT",
    "homepage": "https://github.com/JSPrismarine/JSPrismarine/tree/master/packages/brigadier",
    "devDependencies": {
        "@types/node": "22.10.1",
        "glob": "11.0.0",
        "rimraf": "6.0.1",
        "typescript": "5.5.2",
        "vite": "6.0.2",
        "vitest": "2.1.6"
    },
    "dependencies": {}
}
