{
    "name": "@jsprismarine/logger",
    "version": "0.5.0",
    "description": "JSPrismarine logger and utilities",
    "sideEffects": false,
    "type": "module",
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "main": "./dist/index.cjs.cjs",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "module": "./dist/index.es.js",
            "require": "./dist/index.cjs.cjs",
            "types": "./dist/index.d.ts"
        },
        "./*": {
            "import": "./dist/*.es.js",
            "module": "./dist/*.es.js",
            "require": "./dist/*.cjs.cjs",
            "types": "./dist/*.d.ts"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "clean": "rimraf dist jsp *.tsbuildinfo vite.config.ts.timestamp-* .tsup .turbo",
        "build": "vite build",
        "build:watch": "vite build --watch",
        "typecheck": "tsc -noEmit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JSPrismarine/JSPrismarine.git"
    },
    "keywords": [
        "jsprismarine",
        "errors"
    ],
    "author": "JSPrismarine",
    "license": "MIT",
    "homepage": "https://github.com/JSPrismarine/JSPrismarine/tree/master/packages/errors",
    "devDependencies": {
        "@types/node": "20.12.7",
        "glob": "10.4.4",
        "rimraf": "5.0.9",
        "typescript": "5.5.2",
        "vite": "5.3.3",
        "vitest": "2.0.1"
    },
    "dependencies": {
        "@jsprismarine/color-parser": "workspace:*",
        "winston-transport": "4.7.0",
        "winston": "3.13.0"
    }
}
