{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@jsprismarine/prismarine",
    "sideEffects": false,
    "type": "module",
    "version": "0.1.1",
    "description": "Dedicated Minecraft Bedrock Edition server written in TypeScript",
    "prettier": "@nordcom/prettier",
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": [
                "./src/index.ts",
                "./dist/index.es.js"
            ],
            "module": "./dist/index.es.js",
            "require": "./dist/index.cjs.js",
            "types": "./dist/index.d.ts"
        },
        "./*": {
            "import": "./dist/*.es.js",
            "module": "./dist/*.es.js",
            "require": "./dist/*.cjs.js",
            "types": "./dist/*.d.ts"
        },
        "./package.json": "./package.json"
    },
    "publishConfig": {
        "access": "public"
    },
    "funding": "https://github.com/sponsors/JSPrismarine",
    "homepage": "https://github.com/JSPrismarine/JSPrismarine/tree/master/packages/prismarine",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JSPrismarine/JSPrismarine.git"
    },
    "scripts": {
        "clean": "rimraf dist jsp tsconfig.tsbuildinfo",
        "build": "vite build",
        "typecheck": "tsc -noEmit"
    },
    "keywords": [
        "javascript",
        "typescript",
        "minecraft",
        "mcpe",
        "bedrock",
        "be"
    ],
    "author": "JSPrismarine",
    "license": "MPL-2.0",
    "bugs": {
        "url": "https://github.com/JSPrismarine/JSPrismarine/issues"
    },
    "devDependencies": {
        "@types/graceful-fs": "4.1.9",
        "@types/node": "20.12.2",
        "glob": "10.3.12",
        "rimraf": "5.0.5",
        "typescript": "5.4.3",
        "vite": "5.2.7",
        "vitest": "1.4.0"
    },
    "dependencies": {
        "@jsprismarine/color-parser": "workspace:*",
        "@jsprismarine/errors": "workspace:*",
        "@jsprismarine/jsbinaryutils": "5.5.3",
        "@jsprismarine/nbt": "workspace:*",
        "@jsprismarine/raknet": "workspace:*",
        "fflate": "0.8.2",
        "graceful-fs": "4.2.11",
        "node-fetch": "3.3.2",
        "simplex-noise": "4.0.1",
        "yaml": "2.4.1"
    }
}
