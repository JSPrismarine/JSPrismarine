{
    "name": "@jsprismarine/server",
    "version": "0.3.0",
    "description": "Minecraft: Bedrock Edition RakNet client",
    "sideEffects": false,
    "type": "module",
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "module": "./dist/Server.es.js",
    "types": "./dist/Server.d.ts",
    "exports": {
        ".": {
            "import": "./dist/Server.es.js",
            "module": "./dist/Server.es.js",
            "require": "./dist/Server.cjs.js",
            "types": "./dist/Server.d.ts"
        },
        "./*": {
            "import": "./dist/*.es.js",
            "module": "./dist/*.es.js",
            "require": "./dist/*.cjs.js",
            "types": "./dist/*.d.ts"
        },
        "./package.json": "./package.json"
    },
    "funding": "https://github.com/sponsors/JSPrismarine",
    "scripts": {
        "clean": "rimraf dist jsp tsconfig.tsbuildinfo",
        "build": "vite build",
        "typecheck": "tsc -noEmit",
        "start": "cross-env NODE_ENV=production node ./dist/Server.js --unhandled-rejections=strict",
        "dev": "cross-env NODE_ENV=development JSP_DIR=jsp nodemon -I --quiet --watch '../**/src/**' --ext 'ts' --ignore '../**/*.d.ts' --exec 'vite-node ./src/Server.ts' | pino-pretty -L 10"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JSPrismarine/JSPrismarine.git"
    },
    "keywords": [
        "nodejs",
        "jsprismarine",
        "server",
        "bedrock"
    ],
    "author": "JSPrismarine",
    "license": "MIT",
    "homepage": "https://github.com/JSPrismarine/JSPrismarine/tree/master/packages/client",
    "devDependencies": {
        "@types/node": "20.12.4",
        "cross-env": "7.0.3",
        "glob": "10.3.12",
        "pino-pretty": "11.0.0",
        "rimraf": "5.0.5",
        "tsx": "4.7.1",
        "typescript": "5.4.3",
        "vite-node": "1.4.0",
        "vite": "5.2.8",
        "vitest": "1.4.0"
    },
    "dependencies": {
        "@jsprismarine/prismarine": "workspace:*",
        "@jsprismarine/raknet": "workspace:*"
    }
}
